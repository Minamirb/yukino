<!DOCTYPE html>
<html>
<head>
  <title>Yukino</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if current_auth %>
  <%= current_auth.provider %>でログイン中&nbsp;<%= link_to 'ログアウト', sessions_path, method: :delete%>
<% else %>
  <%= link_to "Twitterでログイン", "/auth/twitter" %>
<% end %>
<%= yield %>

</body>
</html>
